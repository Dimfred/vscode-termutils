{
    "name": "termutils",
    "displayName": "termutils",
    "description": "A collection of utils for the integrated terminal",
    "version": "0.0.2",
    "publisher": "dimfred",
    "engines": {
        "vscode": "^1.97.0"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/dimfred/vscode-termutils"
    },
    "activationEvents": [
        "onStartupFinished"
    ],
    "main": "./dist/extension.js",
    "contributes": {
        "commands": [
            {
                "command": "termutils.focusTerminalByName",
                "title": "termutils: focus a terminal by name"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install"
    },
    "devDependencies": {
        "@types/node": "^14.14.6",
        "@types/vscode": "^1.97.0",
        "esbuild": "^0.25.0",
        "typescript": "^4.0.3",
        "vsce": "^2.15.0",
        "vscode": "^1.1.36"
    }
}